#include <stdio.h>
#include <stdbool.h>

bool hasDistinctDigits(int year) {
    int digits[10] = {0};
    while (year > 0) {
        int digit = year % 10;
        if (digits[digit]) {
            return false;
        }
        digits[digit] = 1;
        year /= 10;
    }
    return true;
}

int main() {
    int y;
    scanf("%d", &y);

    while (1) {
        y++; // Increment the year
        if (hasDistinctDigits(y)) {
            printf("%d\n", y);
            break;
        }
    }
    
    return 0;
}
